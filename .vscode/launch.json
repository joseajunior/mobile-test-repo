{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Suite (no refactor)",
            "request": "launch",
            "cwd": "^\"\\${workspaceFolder}\"",
            "target": "^\"\\${file}\"",
            "terminal": "integrated",
            "env": {},
            "args": [
                "--exclude",
                "test",
                "--timestampoutputs",
                "--loglevel",
                "TRACE",
                "--report",
                "None",
                "--output",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_output.xml",
                "--log",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_log.html",
                "--removekeywords",
                "NAME:set log level"
            ]
        },
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Tag",
            "request": "launch",
            "cwd": "^\"\\${workspaceFolder}\"",
            "target": "^\"\\${file}\"",
            "terminal": "integrated",
            "env": {},
            "args": [
                "--exclude",
                "test",
                "--timestampoutputs",
                "--loglevel",
                "TRACE",
                "--report",
                "None",
                "--output",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_output.xml",
                "--log",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_log.html",
                "--removekeywords",
                "NAME:set log level",
                "--include",
                "checked",
                "--variable",
                "local:True"
            ]
        },
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework Lambda Android: Tag",
            "request": "launch",
            "cwd": "^\"\\${workspaceFolder}\"",
            "target": "^\"\\${file}\"",
            "terminal": "integrated",
            "env": {},
            "args": [
                "--variable",
                "version:12",
                "--variable",
                "platformName:Android",
                "--variable",
                "deviceName:'Galaxy S22 5G'",
                "--variable",
                "isRealMobile:true",
                "--variable",
                "visual:false",
                "--variable",
                "network:false",
                "--variable",
                "console:false",
                "--exclude",
                "test",
                "--timestampoutputs",
                "--loglevel",
                "TRACE",
                "--report",
                "None",
                "--output",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_output.xml",
                "--log",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_log.html",
                "--removekeywords",
                "NAME:set log level",
                "--include",
                "checked",
                "--variable",
                "local:False"
            ]
        },
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework Lambda iOS: Tag",
            "request": "launch",
            "cwd": "^\"\\${workspaceFolder}\"",
            "target": "^\"\\${file}\"",
            "terminal": "integrated",
            "env": {},
            "args": [
                "--variable",
                "version:16",
                "--variable",
                "platformName:iOS",
                "--variable",
                "deviceName:'iPhone 13'",
                "--variable",
                "isRealMobile:true",
                "--variable",
                "visual:false",
                "--variable",
                "network:false",
                "--variable",
                "console:false",
                "--exclude",
                "test",
                "--timestampoutputs",
                "--loglevel",
                "TRACE",
                "--report",
                "None",
                "--output",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_output.xml",
                "--log",
                "${workspaceFolder}${pathSeparator}reports${pathSeparator}${fileBasenameNoExtension}${pathSeparator}${fileBasenameNoExtension}_log.html",
                "--removekeywords",
                "NAME:set log level",
                "--include",
                "checked",
                "--variable",
                "local:False"
            ]
        }
    ]
}